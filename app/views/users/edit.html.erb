<% presenter = Users::EditPresenter.new(self) %>

<% content_for(:head) do %>
  <title><%= presenter.page_title %> | t("views.users.delete_account.edit_user")</title>
  <%= stylesheet_pack_tag 'UserEditPack', media: 'all' %>
<% end %>

<% content_for(:tail) do %>
