# Ignore bundler config.
.bundle/config

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep
/tmp/storage/*
!/tmp/storage/
!/tmp/storage/.keep

/public/assets
/public/uploads
**/node_modules
coverage
tags
.tags*

# Ignore IDE-specific files.
/.idea/
/.vscode
/.zed

# Ignore OS-specific files.
*.DS_Store

# Ignore dotenv files.
.env*

# Ignore public packs.
/public/packs
/public/packs-test

# Ignore these associated with reason-react and bsb
.merlin
lib/bs/
npm-debug.log
.DS_Store
.bsb.lock
*.bs.js

# Ignore the examples file that tracks Rspec runs.
examples.txt

# Ignore .graphql_ppx_cache
.graphql_ppx_cache

# Ignore logs from lerna
lerna-debug.log

# Ignore test flipper store
flipper.pstore

# Ignore JSON locales generated by i18n-js.
/app/frontend/locales/*.json

# Ignore Parcel's cache
.parcel-cache

# Yarn
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/sdks
!.yarn/versions

# Vite Ruby
/public/vite*
# Vite uses dotenv and suggests to ignore local-only env files. See
# https://vitejs.dev/guide/env-and-mode.html#env-files
*.local

# Crontab generated by generate_crontab task
/crontab

config/settings.local.yml
config/settings/*.local.yml
config/environments/*.local.yml
