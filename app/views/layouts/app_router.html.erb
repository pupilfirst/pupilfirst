<% presenter = ::Layouts::AppRouterPresenter.new(self, @course) %>

<% content_for(:head) do %>
  <%= stylesheet_pack_tag 'AppRouterPack', media: 'all' %>
<% end %>

<% content_for(:wrapper) do %>
  <script id="app-router-data" type="application/json">
    <%= presenter.props_to_json.html_safe %>
  </script>
  <div id="app-router"></div>
<% end %>

<% content_for(:tail) do %>
  <%= javascript_pack_tag 'AppRouterPack', nonce: true %>
<% end %>

<%= render template: 'layouts/tailwind' %>
