<% presenter = Schools::Courses::IndexPresenter.new(self, @course) %>

<% content_for(:head) do %>
  <title><%= current_school.name %> | <%= t(".title") %></title>
  <%= stylesheet_pack_tag 'SchoolsCoursesIndexPack', media: 'all' %>
<% end %>

<% content_for(:tail) do %>
  <%= javascript_pack_tag 'SchoolsCoursesIndexPack', nonce: true %>
<% end %>

<script id="course-editor__props" type="application/json">
  <%= presenter.props_to_json.html_safe %>
</script>
<div id="course-editor" class="flex flex-1 h-full"></div>
