<% content_for(:head) do %>
  <title><%= @course.name %> | <%= t(".title") %> </title>
<% end %>
<div id="schoolrouter-innerpage-data">

</div>

<% content_for(:tail) do %>
  <%= javascript_tag(nonce: true) do %>
     window.onload = function() {
       document.getElementById("schoolrouter-innerpage").innerHTML = document.getElementById("schoolrouter-innerpage-data").innerHTML;
       document.getElementById("schoolrouter-innerpage-data").remove();
    }
  <% end %>
<% end %>
