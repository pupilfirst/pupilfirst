Dockerfile
.dockerignore

# Ignore anything vendored locally.
vendor

# Ignore all logfiles and tempfiles.
log
tmp

# Ignore uploaded files in development.
storage

public/assets
public/uploads
node_modules
app/javascript/packages/*/node_modules
coverage
tags
.tags*

# Ignore IDE-specific files.
.idea
.vscode

# Ignore OS-specific files.
*.DS_Store

# Ignore dotenv files.
.env*

# Ignore public packs.
public/packs
public/packs-test
yarn-*.log*
.yarn-integrity

# Ignore these associated with reason-react and bsb
.merlin
lib/bs
npm-debug.log
.DS_Store
.bsb.lock
*.bs.js

# Ignore the examples file that tracks Rspec runs.
examples.txt

# Ignore .graphql_ppx_cache
.graphql_ppx_cache

# Ignore logs from lerna
lerna-debug.log

# Ignore test flipper store
flipper.pstore

# Ignore JSON locales generated by i18n-js.
app/javascript/locales.json

# Ignore git's files.
.git
